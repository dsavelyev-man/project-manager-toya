{
  "name": "database",
  "version": "0.0.1",
  "description": "prisma shared",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "db:generate": "dotenv -e ../../.env prisma generate",
    "db:migrate:dev": "dotenv -e ../../.env prisma migrate dev",
    "db:migrate:prod": "dotenv -e ../../.env prisma migrate prod"
  },
  "main": "dist/index.ts",
  "devDependencies": {
    "dotenv-cli": "^7.3.0",
    "prisma": "^5.6.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0"
  }
}
